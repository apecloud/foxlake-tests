statement ok
CREATE STORAGE IF NOT EXISTS tmpstorage
AT URI '{{.STORAGE_PROVIDER}}://{{.BUCKET}}/{{.STORAGE_PATH}}'
ENDPOINT = '{{.ENDPOINT}}'
CREDENTIALS(ACCESS_KEY_ID='{{.ACCESS_KEY_ID}}' SECRET_ACCESS_KEY='{{.SECRET_ACCESS_KEY}}');

statement ok
DROP STORAGE IF EXISTS tmpstorage;

