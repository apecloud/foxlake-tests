# Test on non-append-only table

statement ok
DROP TABLE IF EXISTS t_insert_all_data_types;

statement ok
CREATE TABLE IF NOT EXISTS t_insert_all_data_types (
  id BIGINT                                                             DEFAULT 0,
  c_bit BIT                                                             DEFAULT 1,
  c_bit_64 BIT(64)                                                      DEFAULT 168134386088498496,
  c_bool BOOL                                                           DEFAULT 0,
  c_boolean BOOLEAN                                                     DEFAULT 1,
  c_tinyint TINYINT                                                     DEFAULT -128,
  c_tinyint_1 TINYINT(1)                                                DEFAULT 1,
  c_tinyint_unsigned TINYINT UNSIGNED                                   DEFAULT 255,
  c_tinyint_zerofill TINYINT ZEROFILL                                   DEFAULT 0123,
  c_smallint SMALLINT                                                   DEFAULT -32768,
  c_smallint_1 SMALLINT(1)                                              DEFAULT 1,
  c_smallint_unsigned SMALLINT UNSIGNED                                 DEFAULT 65535,
  c_smallint_zerofill SMALLINT ZEROFILL                                 DEFAULT 012345,
  c_mediumint MEDIUMINT                                                 DEFAULT -8388608,
  c_mediumint_1 MEDIUMINT(1)                                            DEFAULT 1,
  c_mediumint_unsigned MEDIUMINT UNSIGNED                               DEFAULT 16777215,
  c_mediumint_zerofill MEDIUMINT ZEROFILL                               DEFAULT 01234567,
  c_int INT                                                             DEFAULT -2147483648,
  c_int_1 INT(1)                                                        DEFAULT 1,
  c_int_unsigned INT UNSIGNED                                           DEFAULT 4294967295,
  c_int_zerofill INT ZEROFILL                                           DEFAULT 0123456789,
  c_integer INTEGER                                                     DEFAULT -2147483648,
  c_integer_1 INTEGER(1)                                                DEFAULT 1,
  c_integer_unsigned INTEGER UNSIGNED                                   DEFAULT 4294967295,
  c_integer_zerofill INTEGER ZEROFILL                                   DEFAULT 0123456789,
  c_bigint BIGINT                                                       DEFAULT -9223372036854775806,
  c_bigint_1 BIGINT(1)                                                  DEFAULT 1,
  c_bigint_unsigned BIGINT UNSIGNED                                     DEFAULT 18446744073709551615,
  c_bigint_zerofill BIGINT ZEROFILL                                     DEFAULT 01234567890123456789,
  c_decimal DECIMAL                                                     DEFAULT 123.45,
  c_decimal_5 DECIMAL(5)                                                DEFAULT 123.45,
  c_decimal_5_2 DECIMAL(5, 2)                                           DEFAULT 123.45,
  c_decimal_unsigned DECIMAL UNSIGNED                                   DEFAULT 123.45,
  c_decimal_zerofill DECIMAL ZEROFILL                                   DEFAULT 012345.67,
  c_dec DEC                                                             DEFAULT 123.45,
  c_dec_5 DEC(5)                                                        DEFAULT 12345,
  c_dec_5_2 DEC(5, 2)                                                   DEFAULT 123.45,
  c_dec_unsigned DEC UNSIGNED                                           DEFAULT 123.45,
  c_dec_zerofill DEC ZEROFILL                                           DEFAULT 12345,
  c_numeric NUMERIC                                                     DEFAULT 123.45,
  c_numeric_5 NUMERIC(5)                                                DEFAULT 12345,
  c_numeric_5_2 NUMERIC(5, 2)                                           DEFAULT 123.45,
  c_numeric_unsigned NUMERIC UNSIGNED                                   DEFAULT 123.45,
  c_numeric_zerofill NUMERIC ZEROFILL                                   DEFAULT 12345,
  c_fixed FIXED                                                         DEFAULT 123.45,
  c_fixed_5 FIXED(5)                                                    DEFAULT 12345,
  c_fixed_5_2 FIXED(5, 2)                                               DEFAULT 123.45,
  c_fixed_unsigned FIXED UNSIGNED                                       DEFAULT 123.45,
  c_fixed_zerofill FIXED ZEROFILL                                       DEFAULT 12345,
  c_float FLOAT                                                         DEFAULT 123.45,
  c_float_5 FLOAT(5)                                                    DEFAULT 12345,
  c_float_5_2 FLOAT(5, 2)                                               DEFAULT 123.45,
  c_float_unsigned FLOAT UNSIGNED                                       DEFAULT 123.45,
  c_float_zerofill FLOAT ZEROFILL                                       DEFAULT 12345,
  c_double DOUBLE                                                       DEFAULT 123.45,
  c_double_5_2 DOUBLE(5, 2)                                             DEFAULT 123.45,
  c_double_unsigned DOUBLE UNSIGNED                                     DEFAULT 123.45,
  c_double_zerofill DOUBLE ZEROFILL                                     DEFAULT 123.45,
  c_double_precision DOUBLE PRECISION                                   DEFAULT 123.45,
  c_double_precision_5_2 DOUBLE PRECISION(5, 2)                         DEFAULT 123.45,
  c_double_precision_unsigned DOUBLE PRECISION UNSIGNED                 DEFAULT 123.45,
  c_double_precision_zerofill DOUBLE PRECISION ZEROFILL                 DEFAULT 12345,
  c_real REAL                                                           DEFAULT 123.45,
  c_real_5_2 REAL(5, 2)                                                 DEFAULT 123.45,
  c_real_unsigned REAL UNSIGNED                                         DEFAULT 123.45,
  c_real_zerofill REAL ZEROFILL                                         DEFAULT 123.45,
  c_date DATE                                                           DEFAULT "2023-05-19",
  c_datetime DATETIME                                                   DEFAULT "2023-05-19 12:34:56",
  c_datetime_6 DATETIME(6)                                              DEFAULT "2023-05-19 12:34:56.789",
  c_timestamp TIMESTAMP                                                 DEFAULT "2023-05-19 12:34:56",
  c_timestamp_6 TIMESTAMP(6)                                            DEFAULT "2023-05-19 12:34:56.789",
  c_time TIME                                                           DEFAULT "12:34:56",
  c_time_6 TIME(6)                                                      DEFAULT "12:34:56.789",
  c_year YEAR                                                           DEFAULT 2023,
  c_year_4 YEAR(4)                                                      DEFAULT 2023,
  c_char CHAR                                                           DEFAULT "A",
  c_national_char NATIONAL CHAR                                         DEFAULT "B",
  c_char_8 CHAR(8)                                                      DEFAULT "ABCDEFGH",
  c_char_character_set_utf8 CHAR CHARACTER SET utf8                     DEFAULT "I",
  c_char_collate_utf8_bin CHAR COLLATE utf8_bin                         DEFAULT "Q",
  c_national_varchar_8 NATIONAL VARCHAR(8)                              DEFAULT "ABCDEFGH",
  c_varchar_8 VARCHAR(8)                                                DEFAULT "JKLMNOPQ",
  c_varchar_8_character_set_utf8 VARCHAR(8) CHARACTER SET utf8          DEFAULT "RSTUVWXY",
  c_varchar_8_collate_utf8_bin VARCHAR(8) COLLATE utf8_bin              DEFAULT "RSTUVWXY",
  c_binary BINARY                                                       DEFAULT "b",
  c_binary_8 BINARY(8)                                                  DEFAULT "bin8",
  c_varbinary_8 VARBINARY(8)                                            DEFAULT "vbin8",
  c_tinyblob TINYBLOB                                                   DEFAULT NULL,
  c_blob BLOB                                                           DEFAULT NULL,
  c_blob_8 BLOB(8)                                                      DEFAULT NULL,
  c_mediumblob MEDIUMBLOB                                               DEFAULT NULL,
  c_longblob LONGBLOB                                                   DEFAULT NULL,
  c_tinytext TINYTEXT                                                   DEFAULT NULL,
  c_tinytext_character_set_utf8 TINYTEXT CHARACTER SET utf8             DEFAULT NULL,
  c_tinytext_collate_utf8_bin TINYTEXT COLLATE utf8_bin                 DEFAULT NULL,
  c_text TEXT                                                           DEFAULT NULL,
  c_text_8 TEXT(8)                                                      DEFAULT NULL,
  c_text_character_set_utf8 TEXT CHARACTER SET utf8                     DEFAULT NULL,
  c_text_collate_utf8_bin TEXT COLLATE utf8_bin                         DEFAULT NULL,
  c_mediumtext MEDIUMTEXT                                               DEFAULT NULL,
  c_mediumtext_character_set_utf8 MEDIUMTEXT CHARACTER SET utf8         DEFAULT NULL,
  c_mediumtext_collate_utf8_bin MEDIUMTEXT COLLATE utf8_bin             DEFAULT NULL,
  c_longtext LONGTEXT                                                   DEFAULT NULL,
  c_longtext_character_set_utf8 LONGTEXT CHARACTER SET utf8             DEFAULT NULL,
  c_longtext_collate_utf8_bin LONGTEXT COLLATE utf8_bin                 DEFAULT NULL,
  c_enum ENUM('value1', 'value2')                                       DEFAULT "value1",
  c_enum_character_set_utf8 ENUM('value1', 'value2') CHARACTER SET utf8 DEFAULT "value1",
  c_enum_collate_utf8_bin ENUM('value1', 'value2') COLLATE utf8_bin     DEFAULT "value2",
  c_set SET('value1', 'value2')                                         DEFAULT "value1",
  c_set_character_set_utf8 SET('value1', 'value2') CHARACTER SET utf8   DEFAULT "value1",
  c_set_collate_utf8_bin SET('value1', 'value2') COLLATE utf8_bin       DEFAULT "value2",
  c_json JSON                                                           DEFAULT NULL
);

